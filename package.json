{
  "private": true,
  "engines": {
    "node": "11.14.0"
  },
  "scripts": {
    "webpack-dev-server": "webpack-dev-server --config webpack.config.dev.js",
    "express-server": "nodemon --exec babel-node -- server/app.js -w server --presets env --plugins transform-object-rest-spread",
    "dev": "concurrently 'npm run webpack-dev-server' 'npm run express-server'",
    "build-frontend": "webpack --config webpack.config.prod.js",
    "build-server": "babel server -d server-dist --ignore node_modules --presets env --plugins transform-object-rest-spread,transform-runtime",
    "build": "npm run build-frontend; npm run build-server; git add .; git commit -m \"bundle js\"",
    "start": "node server-dist/app.js",
    "postinstall": "cd server-dist && npm install"
  },
  "dependencies": {
    "apollo-boost": "^0.1.13",
    "babel-runtime": "^6.26.0",
    "classnames": "2.2.5",
    "express": "^4.17.3",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "history": "^4.7.2",
    "knex": "^0.14.3",
    "object.entries": "^1.0.4",
    "object.values": "^1.0.4",
    "pg": "^7.4.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-apollo": "^2.1.11",
    "react-dom": "15.4.2",
    "react-redux": "5.0.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "react-tap-event-plugin": "2.0.1",
    "react-transition-group": "^2.2.1",
    "redux": "3.6.0",
    "redux-logger": "2.8.2",
    "redux-thunk": "2.2.0",
    "scroll-to-element": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "6.23.0",
    "css-loader": "^0.28.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.0",
    "file-loader": "^0.11.2",
    "import-glob-loader": "^1.1.0",
    "kexec": "^3.0.0",
    "node-sass": "^4.5.2",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "webpack": "3.0.0",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0",
    "xml-js": "^1.6.2"
  }
}
